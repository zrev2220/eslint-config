{
  "name": "@zrev2220/eslint-config",
  "version": "1.0.0",
  "description": "My personal ESLint config",
  "main": "index.js",
  "files": [
    ".eslintrc.json",
    "index.js"
  ],
  "scripts": {
    "prepare": "husky install",
    "lint": "run-s prettier-check",
    "lint-fix": "run-s prettier",
    "prettier": "prettier -u --ignore-path .gitignore --write .",
    "prettier-check": "prettier -u --ignore-path .gitignore --check ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zrev2220/eslint-config.git"
  },
  "keywords": [
    "eslint"
  ],
  "author": "https://github.com/zrev2220",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zrev2220/eslint-config/issues"
  },
  "homepage": "https://github.com/zrev2220/eslint-config#readme",
  "dependencies": {
    "@typescript-eslint/parser": "^6.5.0",
    "eslint-import-resolver-typescript": "^3.6.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@zrev2220/prettier-config": "^1.0.0",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
